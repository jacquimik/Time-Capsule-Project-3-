

<div class="form">
  <%= form_for([@child, @event]) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, :autofocus => true %>

    <%= f.label :date %>
    <%= f.date_field :date %>

     <%= f.label :image %>
    <%= f.file_field :image %>
   
    <%= f.label :location %>
    <%= f.text_field :location %>

    <%= f.label :description %>
    <%= f.text_field :description %>
  
    
    <label>
      <%= f.submit %>
    </label>

</div>
  <% end %>